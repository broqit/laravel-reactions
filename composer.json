{
    "name": "broqit/laravel-reactions",
    "description": "Laravel Reactions Package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Borkunov",
            "email": "ya.qnut@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "livewire/livewire": "^3.6.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Broqit\\Laravel\\Reactions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Broqit\\Laravel\\Reactions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Broqit\\Laravel\\Reactions\\ReactionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
